  <!--
    Common Attributes
    
    This module declares many of the common attributes for the XHTML DTD.
    %NS.decl.attrib; is declared in the XHTML Qname module.
  -->
  <define name="id.attrib">
    <optional>
      <attribute name="id">
        <data type="ID"/>
      </attribute>
    </optional>
  </define>
  <define name="class.attrib">
    <optional>
      <attribute name="class">
        <data type="NMTOKENS"/>
      </attribute>
    </optional>
  </define>
  <define name="title.attrib">
    <optional>
      <attribute name="title">
        <ref name="Text.datatype"/>
      </attribute>
    </optional>
  </define>
  <define name="Core.attrib">
    <ref name="XHTML.xmlns.attrib"/>
    <ref name="id.attrib"/>
    <ref name="class.attrib"/>
    <ref name="title.attrib"/>
    <ref name="Core.extra.attrib"/>
  </define>
  <define name="lang.attrib">
    <optional>
      <attribute name="xml:lang">
        <ref name="LanguageCode.datatype"/>
      </attribute>
    </optional>
  </define>
  <define name="dir.attrib">
    <optional>
      <attribute name="dir">
        <choice>
          <value>ltr</value>
          <value>rtl</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <define name="I18n.attrib">
    <ref name="dir.attrib"/>
    <ref name="lang.attrib"/>
  </define>
  <define name="Common.extra.attrib">
    <notAllowed/>
  </define>
  <!-- intrinsic event attributes declared previously -->
  <define name="Common.attrib">
    <ref name="Core.attrib"/>
    <ref name="I18n.attrib"/>
    <ref name="Events.attrib"/>
    <ref name="Common.extra.attrib"/>
  </define>
